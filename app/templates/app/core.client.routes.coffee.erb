ApplicationModules.push (angular) ->
  angular.module("core").config [
    "$stateProvider"
    ($stateProvider) ->
      $stateProvider.state "home",
        url: "/"
        templateUrl: "<%= homeview %>"
        controller: "CoreController"
      $stateProvider.state "about",
          url: "/about"
          templateUrl: "<%= aboutview %>"
          controller: "CoreController"
  ]